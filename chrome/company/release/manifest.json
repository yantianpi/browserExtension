{
  "manifest_version": 2,
  "name": "page html snapshot",
  "version": "1.0",
  "description": "page html snapshot",
  "background": {
    "scripts": ["bootstrap/js/jquery.min.js", "js/md5.min.js", "js/background.js"]
  },
  "permissions": [
    "tabs",
    "notifications",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bootstrap/js/jquery.min.js", "js/contentScript.js"]
    }
  ],
  "options_page": "options.html"
}